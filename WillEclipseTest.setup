        source="http://www.eclipse.org/oomph/setup/BrandingInfo">
      <detail
          key="imageURI">
        <value>https://www.eclipse.org/downloads/images/javaee.png</value>
      </detail>
      <detail
          key="notificationTooltip">
                <value>Sponsor the Eclipse IDE for Enterprise Java and Web Developers</value>
      </detail>
