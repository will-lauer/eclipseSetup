        xsi:type="setup:VariableTask"
        type="BOOLEAN"
        name="enable.groovy"
        defaultValue="true"
        label="Enable Groovy Support"/>
    <setupTask
        xsi:type="setup:VariableTask"
        type="BOOLEAN"
        name="enable.plugins"
        defaultValue="true"
        label="Enable Useful Plugins"/>
